{
    "name": "@typectx/react-client",
    "type": "module",
    "version": "0.0.0",
    "description": "Hook to synchronize React client components with typectx",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "author": "typectx",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typectx/typectx.git"
    },
    "imports": {
        "#*": "./src/*.ts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch"
    },
    "devDependencies": {
        "@config.typectx/eslint": "*",
        "@config.typectx/typescript": "*",
        "@types/react": "^19.2.6",
        "tsup": "^8.5.0",
        "turbo": "^2.5.5"
    },
    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "typectx": "workspace:^"
    }
}
